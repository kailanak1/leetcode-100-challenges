var twoSum = function(nums, target) {
    for(i=0; i < nums.length; i ++){
       for(j = i + 1; j < nums.length; j ++){
           if(nums[i] + nums[j] == target){
               return [i, j]
           }
       }
    }
       
   };

 //make a nested loop to keep track of array[i] and array[j]
 //if the sum of array[i] and array[j] equals the target, return i and j as an array